<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#64BBE9">
    <TextView
        android:id="@+id/text0"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="REGISTRATION NO"
        android:layout_marginTop="50dp"
        android:gravity="center"
        android:textSize="20dp"
        android:textColor="@android:color/black"

        />

    <EditText
        android:id="@+id/edit0"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:background="@android:color/white"
        android:layout_toRightOf="@+id/text0"
        android:hint=" 11717842"
        android:layout_marginTop="50dp"

        />

    <TextView
        android:id="@+id/text1"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Physics"
        android:layout_marginTop="150dp"
        android:gravity="center"
        android:textSize="20dp"
        android:textColor="@android:color/black"


        />

    <EditText
        android:id="@+id/edit1"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:background="@android:color/white"
        android:layout_toRightOf="@+id/text1"
        android:layout_marginTop="150dp"
        android:hint=" Marks/100"/>



    <TextView
        android:id="@+id/text2"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Chemistry"
        android:layout_marginTop="50dp"
        android:gravity="center"
        android:textSize="20dp"
        android:textColor="@android:color/black"
        android:layout_below="@+id/text1"
        />

    <EditText
        android:id="@+id/edit2"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:background="@android:color/white"
        android:layout_toRightOf="@+id/text1"
        android:layout_marginTop="50dp"
        android:hint=" Marks/100"
        android:layout_below="@+id/edit1"/>




    <TextView
        android:id="@+id/text3"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Vedic Maths"
        android:layout_marginTop="50dp"
        android:gravity="center"
        android:textSize="20dp"
        android:textColor="@android:color/black"
        android:layout_below="@+id/text2"
        />

    <EditText
        android:id="@+id/edit3"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:background="@android:color/white"
        android:layout_toRightOf="@+id/text1"
        android:layout_marginTop="50dp"
        android:hint=" Marks/100"
        android:layout_below="@+id/edit2"/>




    <TextView
        android:id="@+id/text4"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="CGPA/TGPA"
        android:layout_marginTop="50dp"
        android:gravity="center"
        android:textSize="20dp"
        android:textColor="@android:color/black"
        android:layout_below="@+id/text3"
        />

    <EditText
        android:id="@+id/edit4"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:background="@android:color/white"
        android:layout_toRightOf="@+id/text2"
        android:layout_marginTop="50dp"
        android:hint=" CGPA/10"
        android:layout_below="@+id/edit3"/>


    <Button
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="submit"
        android:layout_below="@+id/edit4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:background="@drawable/homedesgin"/>





</RelativeLayout>